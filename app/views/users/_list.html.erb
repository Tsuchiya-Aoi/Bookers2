<h2>User info</h2>
<div>
  <%= image_tag user.get_profile_image(100,100) %>
  <h3>name<%= user.name %></h3>
  <h3>introduction<%= user.introduction %></h3>
</div>

  <p><%= link_to "プロフ設定", edit_user_path(user) %></p>


<h2>New book</h2>
  <%= form_with model: book do |f| %>
    <h4>Title</h4>
      <%= f.text_field :title %>
    <h4>Opinion</h4>
      <%= f.text_area :body %><br />
    <%= f.submit 'Create Book' %>
  <% end %>